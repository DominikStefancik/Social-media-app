{
  "name": "social-media-app-server",
  "version": "0.0.0",
  "description": "A simple social media app using Node, Typescript, Graphql, React and MongoDB",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "license": "MIT",
  "keywords": [
    "Node.js",
    "Typescript",
    "Graphql",
    "React",
    "MongoDB"
  ],
  "author": "Dominik Stefancik",
  "scripts": {
    "start": "concurrently --kill-others 'yarn run compile:watch' 'yarn run dev'",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "rebuild": "yarn run clean && yarn run compile",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "compile:watch": "tsc -w",
    "fix": "gts fix",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run lint"
  },
  "dependencies": {
    "@typegoose/typegoose": "^9.1.0",
    "apollo-server-express": "^3.3.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.6.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.9",
    "pino": "^6.13.3",
    "pino-pretty": "^7.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.10.3",
    "@types/pino": "^6.3.11",
    "@types/uuid": "^8.3.1",
    "eslint-plugin-prettier": "^4.0.0",
    "gts": "^3.1.0",
    "module-alias": "^2.2.2",
    "prettier": "^2.4.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3",
    "yarn": "^1.22.15"
  },
  "_moduleAliases": {
    "@local": "dist/src/"
  }
}
